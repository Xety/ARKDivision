{
    "name": "xety/arkdivision",
    "description": "The discuss part of the ARK Division website.",
    "homepage": "https://github.com/Xety/ARKDivision",
    "license": "MIT",
    "type": "project",
    "support": {
        "source": "https://github.com/Xety/ARKDivision",
        "issues": "https://github.com/Xety/ARKDivision/issues"
    },
    "authors": [
        {
            "name": "ZoRo",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "league/color-extractor": "^0.3.2",
        "rinvex/countries": "^7.0",
        "socialiteproviders/discord": "dev-master",
        "spatie/laravel-analytics": "^3.0",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-menu": "^3.0",
        "xetaio/roles": "^7.0",
        "xetaio/xetaravel-editor-md": "^2.0",
        "xetaio/xetaravel-iptraceable": "^2.0",
        "xetaio/xetaravel-mentions": "^3.0",
        "xety/breadcrumbs": "^1.0",
        "anhskohbo/no-captcha": "^3.0",
        "graham-campbell/markdown": "^13.0",
        "kirkbushell/eloquence": "^4.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.10",
        "restcord/restcord": "^0.4",
        "paypal/rest-api-sdk-php": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "phpunit/phpunit": "^8.5",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/filesystem": "^4.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpro/grumphp": "^0.19",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Xetaravel\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

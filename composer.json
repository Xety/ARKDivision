{
    "name": "xety/arkdivision",
    "description": "The discuss part of the ARK Division website.",
    "homepage": "https://github.com/Xety/ARKDivision",
    "license": "MIT",
    "type": "project",
    "support": {
        "source": "https://github.com/Xety/ARKDivision",
        "issues": "https://github.com/Xety/ARKDivision/issues"
    },
    "authors": [
        {
            "name": "ZoRo",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^11.0",
        "kirkbushell/eloquence": "^2.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.7",
        "laravel/framework": "^5.8",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "league/color-extractor": "^0.3.2",
        "league/commonmark-ext-table": "^2.0",
        "league/commonmark-ext-task-list": "^1.0",
        "rinvex/countries": "^6.1",
        "socialiteproviders/discord": "^2.0",
        "spatie/laravel-analytics": "^3.6.4",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-menu": "^3.1",
        "xetaio/recaptcha": "^3.0",
        "xetaio/roles": "^5.8",
        "xetaio/xetaravel-editor-md": "^1.0",
        "xetaio/xetaravel-iptraceable": "^1.0",
        "xetaio/xetaravel-mentions": "^2.0",
        "xety/breadcrumbs": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/filesystem": "^4.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpro/grumphp": "^0.19",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Xetaravel\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
